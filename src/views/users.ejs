<table class="table table-light">
    <% let id = 0; %> 
    <thead class="thead-dark">
        <tr>
          <th scope="col">#</th>
          <th scope="col">Email</th>
          <th scope="col">[Update]</th>
          <th scope="col">Fullname</th>
          <th scope="col">[Delete]</th>
        </tr>
      </thead>
      <tbody>
        <% users.forEach(el => {%>
            <tr>
                <th class="id"><%= id %></th>
                <th class="email"><%= el.email %></th>
                <th class="upd-Button"><%- include('update', { uid: el.id, id, csrf }); %></th>
                <th class="fullName"><%= el.fullName %></th>
                <th class="del-Button"><%- include('delete', { uid: el.id, csrf }) %> </th>
            </tr>
            <% id++; %>
        <% }); %>
      </tbody>
</table>